<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css2?family=Archivo:wght@400;700&display=swap" rel="stylesheet" />

  <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon" />

  <!-- Carousel -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Glide.js/3.4.1/css/glide.core.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Glide.js/3.4.1/css/glide.theme.min.css
">
  <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />

  <!-- Custom StyleSheet -->
  <link rel="stylesheet" href="styles.css" />

  <title> {% block title %}{% endblock title %} </title>
</head>

<body>

    <!-- Header -->
    {% include "z-helpers/header.html.twig" %}
    <!-- End Header -->

    <!-- Carousel -->
    {% if block('carousel') is defined %}
        {% include "z-helpers/carousel.html.twig" %}
    {% endif %}
    <!-- End Carousel -->

    <!-- Main -->
    <main id="main">
        <div class="container">
            {% if block('category') is defined %}
                {% include "z-helpers/category.html.twig" %}
            {% endif %}
            {% if block('bestSeller') is defined %}
                {% include "z-helpers/bestSeller.html.twig" %}
            {% endif %}
            {% if block('services') is defined %}
                {% include "z-helpers/services.html.twig" %}
            {% endif %}
        </div>
        {% if block('temoignage') is defined %}
            {% include "z-helpers/temoignage.html.twig" %}
        {% endif %}
    </main>    
    <!-- End Main -->

    <!-- Footer -->
    <!-- End Footer -->

    <!-- PopUp -->
    <!-- End PopUp -->
    
    <!-- Go To -->
    <!-- End Go To -->


  <!-- Glide Carousel Script -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/Glide.js/3.4.1/glide.min.js"></script>
  <!-- Animate On Scroll -->
  <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>

  <!-- Custom JavaScript -->
  <script src="js/products.js"></script>
  <script src="js/index.js"></script>
  <script src="js/slider.js"></script>

  

</body>

</html>